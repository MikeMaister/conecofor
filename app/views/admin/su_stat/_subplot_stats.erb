<table class="simple head">
  <tbody>
  <tr>
    <th class="big">Plot</th>
    <th>Subplot</th>
    <th class="big">Eucode</th>
    <th class="bigger">Eudesc</th>
    <th class="bigger">Specie</th>
    <th>Copertura</th>
    <th>Individui</th>
  </tr>
  </tbody>
</table>
<div id="ver_scroll">
<table class="simple body">
  <tbody>
  <% for i in 0..@stat_list.size-1 %>
      <tr>
        <td class="big"><%= @stat_list.at(i).plot %></td>
        <td><%= @stat_list.at(i).subplot %></td>
        <td class="big"><%= @stat_list.at(i).eucode %></td>
        <td class="bigger"><%= @stat_list.at(i).eudesc %></td>
        <td class="bigger"><%= @stat_list.at(i).specie %></td>
        <td><%= @stat_list.at(i).copertura %></td>
        <td><%= @stat_list.at(i).individui %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</div>

<p id="download">
  <%= link_to "Download File", @file.relative_path %>
</p>