<table class="comp head">
  <tbody>
  <tr>
    <th>Plot</th>
    <th>Subplot</th>
    <% unless @inout.blank? %>
        <th>In/Out</th>
    <% end %>
    <% unless @priest.blank? %>
        <th>Pri/Est</th>
    <% end %>
    <% unless @cod_strato.blank? %>
        <th>Codice Strato</th>
    <% end %>
    <th class="big">Eucode</th>
    <th class="bigger">Eudesc</th>
    <th class="bigger">Specie</th>
    <th>Copertura</th>
    <th>Individui</th>
  </tr>
  </tbody>
</table>
<div id="ver_scroll_comp">
 <table class="comp body">
  <tbody>
  <% for i in 0..@stat_list.size-1 %>
      <tr>
        <td><%= @stat_list.at(i).plot %></td>
        <td><%= @stat_list.at(i).subplot %></td>
        <% unless @inout.blank? %>
            <td><%= @stat_list.at(i).inout %></td>
        <% end %>
        <% unless @priest.blank? %>
            <td><%= @stat_list.at(i).priest %></td>
        <% end %>
        <% unless @cod_strato.blank? %>
            <td><%= @stat_list.at(i).cod_strato %></td>
        <% end %>
        <td class="big"><%= @stat_list.at(i).eucode %></td>
        <td class="bigger"><%= @stat_list.at(i).eudesc %></td>
        <td class="bigger"><%= @stat_list.at(i).specie %></td>
        <td><%= @stat_list.at(i).copertura %></td>
        <td><%= @stat_list.at(i).individui %></td>
      </tr>
  <% end %>
  </tbody>
</table>
</div>

<p id="download">
  <%= link_to "Download File", @file.relative_path %>
</p>