<% if @import_data.blank?%>
    <%= render :partial => "no_data" %>
<% else %>
    <%= render :partial => "approve_delete" %>

<div id="wrap_result" class="overx">
    <table>
      <tbody>
      <th class="row">Riga</th>
      <th>Data</th>
      <th>ID plot</th>
      <th>Subplot</th>
      <th>In/Out</th>
      <th>Pri/Est</th>
      <th>Note</th>
      <th>Numero Plot</th>
      <th>Copertura Complessiva</th>
      <th>Altezza Arboreo</th>
      <th>Copertura Arboreo</th>
      <th>Altezza Arbustivo</th>
      <th>Copertura Arbustivo</th>
      <th>Altezza Erbaceo</th>
      <th>Copertura Erbaceo</th>
      <th>Copertura Muscinale</th>
      <th>Copertura Lettiera</th>
      <th>Copertura Suolo Nudo</th>
      </tbody>
    </table>
    <div id="ver_scroll">
    <table id="body">
      <tbody>
      <% for i in 0..@import_data.size-1 %>
          <tr>
            <td class="row"><%= i+2 %></td>
            <td><%= @import_data.at(i).data %></td>
            <td><%= @import_data.at(i).id_plot %></td>
            <td><%= @import_data.at(i).subplot %></td>
            <td><%= @import_data.at(i).in_out %></td>
            <td><%= @import_data.at(i).priest %></td>
            <td><%= @import_data.at(i).note %></td>
            <td><%= @import_data.at(i).numero_plot %></td>
            <td><%= @import_data.at(i).copertura_complessiva %></td>
            <td><%= @import_data.at(i).altezza_arboreo %></td>
            <td><%= @import_data.at(i).copertura_arboreo %></td>
            <td><%= @import_data.at(i).altezza_arbustivo %></td>
            <td><%= @import_data.at(i).copertura_arbustivo %></td>
            <td><%= @import_data.at(i).altezza_erbaceo %></td>
            <td><%= @import_data.at(i).copertura_erbaceo %></td>
            <td><%= @import_data.at(i).copertura_muscinale %></td>
            <td><%= @import_data.at(i).copertura_lettiera %></td>
            <td><%= @import_data.at(i).copertura_suolo_nudo %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
    </div>
</div>
<% end %>