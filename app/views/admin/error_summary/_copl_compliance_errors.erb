<% if !@comp.blank? %>
    <table>
      <thead>
      <tr>
        <th>Compliance</th>
      </tr>
      <tr>
        <th>Riga</th>
        <th>Errore</th>
        <th>Creato il</th>
        <th>N.Import</th>
        <th>Plot</th>
        <th>Subplot</th>
        <th>In/Out</th>
        <th>Pri/Est</th>
        <th>Copertura Complessiva</th>
        <th>Altezza Arboreo</th>
        <th>Copertura Arboreo</th>
        <th>Altezza Arbustivo</th>
        <th>Copertura Arbustivo</th>
        <th>Altezza Erbaceo</th>
        <th>Copertura Erbaceo</th>
        <th>Copertura Muscinale</th>
        <th>Copertura Lettiera</th>
        <th>Copertura Suolo Nudo</th>
        <th>Note</th>
        <th>Data</th>
      </tr>
      </thead>
      <tbody>
      <% for i in 0..@comp.size-1 %>
          <tr>
            <td><%= @comp.at(i).row %></td>
            <td><%= @comp.at(i).error %></td>
            <td><%= @comp.at(i).created_at %></td>
            <td><%= @comp.at(i).import_num %></td>
            <td><%= @comp.at(i).plot %></td>
            <td><%= @comp.at(i).subplot %></td>
            <td><%= @comp.at(i).in_out %></td>
            <td><%= @comp.at(i).priest %></td>
            <td><%= @comp.at(i).copertura_complessiva %></td>
            <td><%= @comp.at(i).altezza_arboreo %></td>
            <td><%= @comp.at(i).copertura_arboreo %></td>
            <td><%= @comp.at(i).altezza_arbustivo %></td>
            <td><%= @comp.at(i).copertura_arbustivo %></td>
            <td><%= @comp.at(i).altezza_erbaceo %></td>
            <td><%= @comp.at(i).copertura_erbaceo %></td>
            <td><%= @comp.at(i).copertura_muscinale %></td>
            <td><%= @comp.at(i).copertura_lettiera %></td>
            <td><%= @comp.at(i).copertura_suolo %></td>
            <td><%= @comp.at(i).note %></td>
            <td><%= @comp.at(i).data %></td>
          </tr>
      <% end %>
      </tbody>
    </table>

<% else %>
    <p>
      Nessun errore di tipo compliance presente.
    </p>
<% end %>
