<% unless @import_data.blank?%>
    <div id="oriz_scroll">
    <table id="head">
      <thead>
        <tr class="head">
            <th>Data</th>
            <th>Subplot</th>
            <th>In/Out</th>
            <th>Pri/Est</th>
            <th>Strato</th>
            <th class="spe">Specie</th>
            <th>Copertura Specifica</th>
            <th class="note">Note Rilevamento</th>
            <th class="note">Note Specie Abituale</th>
            <th class="note">Substrate</th>
            <th class="note">Certainty Species Determination</th>
        </tr>
      </thead>
    </table>
    <div id="scroller">
    <table id="body">
      <tbody>
      <% for i in 0..@import_data.size-1 %>
        <tr class="body">
            <td><%= @import_data.at(i).data %></td>
            <td><%= @import_data.at(i).subplot %></td>
            <td><%= @import_data.at(i).in_out %></td>
            <td><%= @import_data.at(i).priest %></td>
            <td><%= @import_data.at(i).codice_strato %></td>
            <td class="spe"><%= @import_data.at(i).descrizione_pignatti %></td>
            <td><%= @import_data.at(i).copertura_specifica.identifier %></td>
            <td class="note"><%= convert_nil(@import_data.at(i).note) %></td>
            <td class="note"><%= convert_nil(@import_data.at(i).habitual_specie_note) %></td>
            <td class="note">
                <% if @import_data.at(i).substrate_type_id.blank? %>
                  <%= convert_nil(@import_data.at(i).substrate_type) %>
                <% else %>
                    <%= @import_data.at(i).substrate_type.code %>
                <% end %>
            </td>
            <td class="note">
                <% if @import_data.at(i).certainty_species_determination_id.blank? %>
                    <%= convert_nil(@import_data.at(i).certainty_species_determination) %>
                <% else %>
                    <%= @import_data.at(i).certainty_species_determination.code %>
                <% end %>
            </td>
        </tr>
      <% end %>
      </tbody>
    </table>
    </div>
    </div>
<% end %>

<p id="download">
  <%= link_to "Download File",@file.relative_path unless @import_data.blank? %>
</p>