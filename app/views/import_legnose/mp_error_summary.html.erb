<h1>Multipleparameter Errors</h1>
<table>
  <tbody>
  <tr>
    <th colspan="11">
      <%= @file.file_name %>
    </th>
  </tr>
  <tr>
    <th>Row</th>
    <th>Error</th>
    <th>Data</th>
    <th>Plot</th>
    <th>Subplot</th>
    <th>Specie</th>
    <th>Copertura</th>
    <th>Altezza</th>
    <th>Et√† Strutturale</th>
    <th>Danni Meccanici</th>
    <th>Danni Parassitari</th>
    <th>Radicanti Esterni</th>
    <th>Note</th>
  </tr>
  <% for i in 0..@mp_err.size-1%>
      <tr>
        <td><%= @mp_err.at(i).row %></td>
        <td><%= @mp_err.at(i).error %></td>
        <td><%= @mp_err.at(i).data %></td>
        <td><%= @mp_err.at(i).plot %></td>
        <td><%= @mp_err.at(i).subplot %></td>
        <td><%= @mp_err.at(i).specie %></td>
        <td><%= @mp_err.at(i).copertura %></td>
        <td><%= @mp_err.at(i).altezza %></td>
        <td><%= @mp_err.at(i).eta_strutturale %></td>
        <td><%= @mp_err.at(i).danni_meccanici %></td>
        <td><%= @mp_err.at(i).danni_parassitari %></td>
        <td><%= @mp_err.at(i).radicanti_esterni %></td>
        <td><%= @mp_err.at(i).note %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<% unless @mp_gbe.blank? %>
    <h2>Global Errors</h2>
    <table>
      <tbody>
      <tr>
        <th>
          <%= @file.file_name %>
        </th>
      </tr>
      <tr>
        <th>Error</th>
      </tr>
      <% for i in 0..@mp_gbe.size-1%>
          <tr>
            <td><%= @mp_gbe.at(i).error %></td>
          </tr>
      <% end %>
      <tr>
        <td>
          <% if session[:mp_error] == false && session[:mp_warning] == true  %>
              <%= button_to "Forza dati", :action => "force_input" %>
          <% end %>
        </td>
      </tr>
      </tbody>
    </table>
<% end %>

<p>
  <%= button_to_function("Print this Page", "javascript:print()") %>
</p>